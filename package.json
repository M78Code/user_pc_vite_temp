{
    "name": "user-pc-vite",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "divider--1--1": "echo  -------------------------通用基础命令  开始----------------------------- ",
        "export-node-env": "node ./job/export-node-env.js",
        "env-dev": "node ./job/export-node-env.js development",
        "env-prod": "node ./job/export-node-env.js production",
        "copy-oss-output": "node ./job/copy-oss.js output",
        "copy-oss-dist": "node ./job/copy-oss.js dist",
        "copy-oss-backup": "node ./job/copy-oss.js backup",
        "remove-output": "node ./job/remove-output.js",
        "remove-dist": "node ./job/remove-dist.js",
        "remove-dist-assets": "node ./job/remove-dist-assets.js",
        "copy-html": "node ./job/copy-html.js",
        "export-html": "node ./job/export-html.js",
        "divider--1--2": "echo  -------------------------通用基础命令  结束----------------------------- ",
        "divider--2--1": "echo  -------------------------服务端配置拆分命令  开始----------------------------- ",
        "export-build-in-config": "node ./job/export-build-in-config.js",
        "export-full-doc": "node ./job/export-full-doc.js",
        "export-merchant-config": "node ./job/export-merchant-config.js",
        "export-theme-config": "node ./job/export-theme-config.js",
        "export-i18n-config": "node ./job/export-i18n-config.js",
        "export-component-config": "node ./job/export-component-config.js",
        "export-css-config": "node ./job/export-css-config.js",
        "export-assets-config": "node ./job/export-assets-config.js",
        "export-js-config": "node ./job/export-js-config.js",
        "resolve-merchant-config": "run-s export-merchant-config export-theme-config export-i18n-config export-component-config  export-js-config  export-css-config  export-assets-config",
        "divider--2--2": "echo  --------------------------服务端配置拆分命令  结束----------------------------- ",
        "divider--11--1": "echo  -------------------------常规客户端项目  开始----------------------------- ",
        "export-build-version-client": "node ./job/export-build-version-client.js",
        "dev-self-use-version-inner": "vite    --config  ./entries/self-use-version/self-use-version.vite.config.js",
        "build-self-use-version-inner": "vite  build  --config  ./entries/self-use-version/self-use-version.vite.config.js   --mode  production",
        "dev-self-use-version": "run-s remove-output  env-dev  export-build-version-client   remove-dist  copy-oss-backup   copy-oss-output  export-merchant-config  export-build-in-config  resolve-merchant-config export-full-version-close  export-html dev-self-use-version-inner",
        "build-self-use-version": "run-s remove-output env-prod  export-build-version-client   remove-dist  copy-oss-backup copy-oss-output   export-merchant-config  export-build-in-config resolve-merchant-config  export-full-version-close  export-html build-self-use-version-inner  copy-html copy-oss-dist  remove-dist-assets ",
        "divider--11--2": "echo  -------------------------常规客户端项目  结束----------------------------- ",
        "divider--12--1": "echo  -------------------------活动项目  开始----------------------------- ",
        "export-build-version-activity": "node ./job/export-build-version-activity.js",
        "dev-activity-inner": "vite    --config  ./entries/activity/activity.vite.config.js",
        "build-activity-inner": "vite   build --config  ./entries/activity/activity.vite.config.js  --mode  production",
        "dev-activity": "run-s remove-output  env-dev  export-build-version-activity   remove-dist copy-oss-backup   copy-oss-output  export-build-in-config  export-html dev-activity-inner",
        "build-activity": "run-s remove-output env-prod  export-build-version-activity   remove-dist copy-oss-backup   copy-oss-output export-build-in-config  export-html   build-activity-inner   copy-html copy-oss-dist   remove-dist-assets",
        "divider--12--2": "echo  -------------------------活动项目  结束----------------------------- ",
        "divider--13--1": "echo  -------------------------动画项目  开始----------------------------- ",
        "export-build-version-animation": "node ./job/export-build-version-animation.js",
        "dev-animation-inner": "vite    --config  ./entries/animation/animation.vite.config.js",
        "build-animation-inner": "vite   build --config  ./entries/animation/animation.vite.config.js  --mode  production",
        "dev-animation": "run-s remove-output  env-dev  export-build-version-animation   remove-dist copy-oss-backup   copy-oss-output  export-build-in-config export-html dev-animation-inner",
        "build-animation": "run-s remove-output env-prod  export-build-version-animation   remove-dist copy-oss-backup   copy-oss-output export-build-in-config export-html  build-animation-inner copy-html copy-oss-dist  remove-dist-assets",
        "divider--13--2": "echo  -------------------------动画项目  结束----------------------------- ",
        "divider--14--1": "echo  -------------------------SDK 生成 项目  开始----------------------------- ",
        "export-build-version-client-sdk-dev": "node ./job/export-build-version-client-sdk-dev.js",
        "export-build-version-client-sdk-build": "node ./job/export-build-version-client-sdk-build.js",
        "dev-client-sdk-inner": "vite    --config  ./entries/client-sdk-dev/client-sdk-dev.vite.config.js",
        "dev-client-sdk": "run-s remove-output  env-dev  export-build-version-client-sdk-dev   remove-dist copy-oss-backup   copy-oss-output  export-build-in-config   dev-client-sdk-inner",
        "build-client-sdk-inner": "vite  build    --config  ./entries/client-sdk-build/client-sdk-build.vite.config.js   --mode  production ",
        "build-client-sdk": "run-s  remove-output env-prod  export-build-version-client-sdk-build   remove-dist  copy-oss-backup copy-oss-output   export-merchant-config  export-build-in-config resolve-merchant-config  export-full-version-close build-client-sdk-inner copy-oss-dist  remove-dist-assets",
        "divider--14--2": "echo  -------------------------SDK 生成 项目  结束----------------------------- ",
        "divider--15--1": "echo  -------------------------SDK 验证 项目  开始----------------------------- ",
        "dev-check-sdk-esm": "vite    --config  ./entries/check-sdk-esm/check-sdk-esm.vite.config.js",
        "dev-check-sdk-umd": "vite    --config  ./entries/check-sdk-umd/check-sdk-umd.vite.config.js",
        "dev-check-sdk-umd-html": "vite    --config  ./entries/check-sdk-umd-html/check-sdk-umd-html.vite.config.js",
        "divider--15--2": "echo  -------------------------SDK 验证 项目  结束----------------------------- ",
        "divider--16--1": "echo  -------------------------模板项目  开始----------------------------- ",
        "export-build-version-template-project": "node ./job/export-build-version-template-project.js",
        "dev-template-project-inner": "vite    --config  ./entries/template-project/template-project.vite.config.js",
        "build-template-project-inner": "vite   build --config  ./entries/template-project/template-project.vite.config.js  --mode  production",
        "dev-template-project": "run-s remove-output  env-dev  export-build-version-template-project   remove-dist copy-oss-backup   copy-oss-output  export-build-in-config dev-template-project-inner",
        "build-template-project": "run-s remove-output env-prod  export-build-version-template-project   remove-dist copy-oss-backup   copy-oss-output export-build-in-config build-template-project-inner copy-html copy-oss-dist  remove-dist-assets",
        "divider--16--2": "echo  -------------------------模板项目  结束----------------------------- ",
        "divider--111--1": "echo  -------------------------其他辅助项目  开始----------------------------- ",
        "dev-development-document-inner": "vite    --config  ./entries/development-document/development-document.vite.config.js",
        "dev-development-document-alert": "echo \"请注意：项目内部开发文档必须前置执行生成全量文档json 命令 : export-full-doc,确保代码内有最新文档\"",
        "dev-development-document": "run-s dev-development-document-alert  export-full-doc dev-development-document-inner",
        "dev-custom-version-inner": "vite    --config  ./entries/custom-version/custom-version.vite.config.js",
        "custom-merchant-config-version-alert": "echo \"请注意：自定义版本必须设置商户配置版本号，必须前置执行变量设置命令,变量名字： MERCHANT-CONFIG-VERSION \"",
        "dev-custom-version": "run-p  custom-merchant-config-version-alert export-merchant-config dev-custom-version-inner",
        "build-custom-version-inner": "vite  build    --config  ./entries/custom-version/custom-version.vite.config.js   --mode  production ",
        "build-custom-version": "run-p custom-merchant-config-version-alert export-merchant-config build-custom-version-inner",
        "dev-full-version-inner": "vite    --config  ./entries/full-version/full-version.vite.config.js",
        "dev-full-version": "run-s  export-full-version-open  dev-full-version-inner",
        "build-full-version": "vite  build    --config  ./entries/full-version/full-version.vite.config.js   --mode  production ",
        "dev-postmessage-test": "vite    --config  ./entries/postmessage-test/postmessage-test.vite.config.js",
        "export-full-version-open": "node ./job/export-full-version-config.js full",
        "export-full-version-close": "node ./job/export-full-version-config.js",
        "divider--111--2": "echo  -------------------------其他辅助项目  结束----------------------------- ",
        "divider-line": "echo \" 此命令无用,用于分割区块 。 上面的是语义化全命令，下面是简写命令。不要混着写。 \" && exit 0",
        "dev": "npm run dev-self-use-version",
        "dev-huodong": "npm run dev-activity",
        "build-huodong": "npm run build-activity",
        "dev-donghua": "npm run dev-animation",
        "build-donghua": "npm run build-animation",
        "dev2": "npm run dev-self-use-version-inner",
        "build": "npm run build-self-use-version",
        "emc": "npm run export-merchant-config",
        "ddd": "npm run dev-development-document",
        "dfv": "npm run dev-full-version",
        "dcv": "npm run dev-custom-version",
        "efvo": "npm run export-full-version-open",
        "efvc": "npm run export-full-version-close",
        "ebca": "npm run  export-build-in-config all",
        "ebc": "npm run  export-build-in-config",
        "echo": "echo \" 此命令无用，查看并选择其他合适命令执行构建 。\" && exit 0"
    },
    "dependencies": {
        "@quasar/extras": "^1.16.5",
        "@reduxjs/toolkit": "^1.9.5",
        "axios": "^1.4.0",
        "calculatorjs": "^1.1.2",
        "clipboard": "^2.0.11",
        "current-device": "^0.10.2",
        "dayjs": "^1.11.10",
        "dexie": "^3.2.4",
        "licia": "^1.38.2",
        "licia-es": "^1.38.2",
        "lodash": "^4.17.21",
        "md5": "^2.3.0",
        "mitt": "^3.0.1",
        "pako": "^2.1.0",
        "quasar": "^2.12.3",
        "redux": "^4.2.1",
        "swrv": "^1.0.4",
        "vue": "^3.3.4",
        "vue-currency-input": "^3.0.5",
        "vue-draggable-resizable": "^2.3.0",
        "vue-i18n": "^9.2.2",
        "vue-router": "^4.2.4",
        "vue3-draggable-resizable": "^1.6.5"
    },
    "devDependencies": {
        "@quasar/vite-plugin": "^1.4.1",
        "@vitejs/plugin-vue": "^4.2.3",
        "colors": "^1.4.0",
        "cross-env": "^7.0.3",
        "current-device": "^0.10.2",
        "http-server": "^14.1.1",
        "npm-run-all": "^4.1.5",
        "randomstring": "^1.3.0",
        "sass": "^1.32.12",
        "vite": "^4.4.5",
        "vue-json-viewer": "^3.0.4"
    }
}
